  "description": "",
  "main": "index.js",
  "scripts": {
    "start_redis": "redis-server --daemonize yes",
    "start_dev_server": "cd server && nodemon index.js",
    "start_pro_server": "cd server && node index.js",
    "start_dev_client": "cd client && webpack serve --config=webpack.config.dev.js --open",
    "start_pro_client": "cd client && webpack --config=webpack.config.prod.js",
    "dev": "concurrently -n 'redis,server,client' 'npm run start_redis' 'npm run start_dev_server' 'npm run start_dev_client'",
    "pro": "concurrently -n 'redis,server,client' -c 'blue,yellow,green' 'npm run start_redis' 'npm run start_pro_server' 'npm run start_pro_client'"
  },
